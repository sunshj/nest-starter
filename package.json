    "@prisma/client": "5.5.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "consola": "^3.2.3",
    "dayjs": "^1.11.10",
    "express": "^4.21.1",
    "multer": "1.4.5-lts.1",
    "nestjs-pino": "^3.5.0",
